{
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" },
    {
      "src": "/assets/(.*\\.webp)",
      "headers": [{ "key": "Content-Type", "value": "image/webp" }]
    },
    {
      "src": "/assets/(.*\\.png)",
      "headers": [{ "key": "Content-Type", "value": "image/png" }]
    },
    {
      "src": "/assets/(.*\\.jpg)",
      "headers": [{ "key": "Content-Type", "value": "image/jpeg" }]
    }
  ]
}
