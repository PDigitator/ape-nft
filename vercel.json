{
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" },
    {
      "src": "/assets/(.*\\.webp)",
      "headers": { "Content-Type": "image/webp" }
    },
    {
      "src": "/assets/(.*\\.png)",
      "headers": { "Content-Type": "image/png" }
    },
    {
      "src": "/assets/(.*\\.jpg)",
      "headers": { "Content-Type": "image/jpeg" }
    }
  ]
}
